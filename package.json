  "dependencies": {
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "eslint": "^8.44.0",
    "eslint-config-xo": "^0.43.1",
    "nodemon": "^2.0.22"
  }
